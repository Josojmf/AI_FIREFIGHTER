# MongoDB Atlas Configuration (will be set by GitHub Actions)
DB_USERNAME=${DB_USERNAME}
DB_PASSWORD=${DB_PASSWORD}
DB_CLUSTER=${DB_CLUSTER}

# Application Configuration
NODE_ENV=production
API_BASE_URL=http://firefighter_backend:5000
# Docker Images (these will be set by GitHub Actions)
FRONTEND_IMAGE=
BACKEND_IMAGE=
BACKOFFICE_IMAGE=

# Security
SESSION_SECRET=firefighter-super-secret-jwt-key-2024
FRONTEND_SECRET_KEY=firefighter-super-secret-jwt-key-2024
BACKOFFICE_SECRET_KEY=firefighter-super-secret-jwt-key-2024
SECRET_KEY=firefighter-super-secret-jwt-key-2024
JWT_SECRET=firefighter-super-secret-jwt-key-2024
JWT_SECRET_KEY=firefighter-super-secret-jwt-key-2024
# Logging
LOG_LEVEL=info

# Performance
MAX_CONNECTIONS=100
TIMEOUT=30000